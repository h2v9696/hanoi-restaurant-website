{
  "extends": [
    "react-app",
    "eslint:recommended"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": [
          "node_modules",
          "src/"
        ]
      }
    }
  },
  "rules": {
    "react-app/jsx-a11y/href-no-hash": "off",
    "no-unused-vars": 1,
    "eol-last": 2
  }
}
